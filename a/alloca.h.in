/* @configure_input@ */
#ifndef a_alloca_h
#define a_alloca_h 1

/* alloca checks AIX requires this to be the first thing in the file.  */
#ifndef __GNUC__
#	if @HAVE_ALLOCA_H@
#		include <alloca.h>
#	else
#		ifdef _AIX
#			pragma alloca
#		elif @OS_TYPE_WIN32@
#			include <malloc.h>
#		else
#			ifndef alloca /* predefined by HP cc +Olibcalls */
				char *alloca ();
#			endif
#		endif
#	endif

#endif

#endif
